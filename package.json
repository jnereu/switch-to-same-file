{
  "name": "switch-to-same-file",
  "displayName": "Switch to Same File",
  "description": "Switch to another file in the workspace with the same name if it's open.",
  "version": "0.0.2",
  "publisher": "Jorge Nereu",
  "engines": {
      "vscode": "^1.7.5"
  },
  "activationEvents": [
    "onCommand:extension.switchToSameFile"
  ],
  "categories": [
      "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.switchToSameFile",
        "title": "Switch to Same File Name"
      }
    ],
    "keybindings": [
      {
        "command": "extension.switchToSameFile",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
      "vscode": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnereu/switch-to-same-file.git" 
  },
  "license": "MIT"
}